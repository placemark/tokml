{
	"name": "@placemarkio/tokml",
	"version": "0.3.5",
	"description": "Convert GeoJSON to KML",
	"main": "dist/tokml.cjs",
	"module": "dist/tokml.es.mjs",
	"typings": "dist/index.d.ts",
	"author": "Tom MacWright",
	"license": "MIT",
	"exports": {
		"require": "./dist/tokml.cjs",
		"types": "./dist/index.d.ts",
		"default": "./dist/tokml.es.mjs"
	},
	"keywords": [
		"kml",
		"geojson"
	],
	"publishConfig": {
		"access": "public"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pnpm lint"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"test": "vitest",
		"lint": "biome check",
		"doc": "typedoc --options typedoc.json",
		"prepublish": "rollup -c rollup.config.mjs && cp CNAME docs/CNAME",
		"release": "standard-version"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@rollup/plugin-node-resolve": "^15.0.1",
		"@rollup/plugin-typescript": "^9.0.2",
		"@tmcw/togeojson": "5.4.0",
		"@types/geojson": "^7946.0.8",
		"eslint": "^8.18.0",
		"rollup": "4.28.1",
		"rollup-plugin-dts": "^5.0.0",
		"rollup-plugin-terser": "^7.0.2",
		"standard-version": "^9.5.0",
		"tslib": "^2.4.0",
		"typedoc": "^0.23.19",
		"typescript": "^4.7.4",
		"unist-builder": "^3.0.0",
		"vitest": "^0.24.4",
		"xast": "^0.1.164",
		"xast-util-to-xml": "^3.0.0",
		"xastscript": "^3.0.0"
	}
}
