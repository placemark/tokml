{
  "name": "@placemarkio/tokml",
  "version": "0.3.2",
  "description": "Convert GeoJSON to KML",
  "main": "dist/tokml.cjs",
  "module": "dist/tokml.es.mjs",
  "typings": "dist/index.d.ts",
  "author": "Tom MacWright",
  "license": "MIT",
  "exports": {
    "require": "./dist/tokml.cjs",
    "default": "./dist/tokml.es.mjs"
  },
  "keywords": [
    "kml",
    "geojson"
  ],
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint . && tsc --noEmit",
    "doc": "typedoc --options typedoc.json",
    "prepare": "rollup -c rollup.config.js && npm run doc && cp CNAME docs/CNAME",
    "release": "standard-version"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@rollup/plugin-typescript": "^8.3.2",
    "@swc/core": "^1.2.172",
    "@swc/jest": "^0.2.20",
    "@tmcw/togeojson": "5.1.2",
    "@types/geojson": "^7946.0.8",
    "@types/jest": "^27.4.1",
    "eslint": "^8.14.0",
    "jest": "^28.0.1",
    "prettier": "^2.6.2",
    "rollup": "^2.70.2",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9.3.2",
    "tslib": "^2.4.0",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.3",
    "unist-builder": "^3.0.0",
    "xast-util-to-xml": "^3.0.0",
    "xastscript": "^3.0.0"
  }
}
