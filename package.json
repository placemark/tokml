{
  "name": "@placemarkio/tokml",
  "version": "0.3.0-0",
  "description": "Convert GeoJSON to KML",
  "main": "dist/tokml.cjs",
  "module": "dist/tokml.esm.js",
  "typings": "dist/index.d.ts",
  "author": "Tom MacWright",
  "license": "MIT",
  "exports": {
    "require": "./dist/tokml.cjs",
    "default": "./dist/tokml.es.js"
  },
  "keywords": [
    "kml",
    "geojson"
  ],
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint . && tsc --noEmit",
    "doc": "typedoc --options typedoc.json",
    "prepare": "rollup -c rollup.config.js && npm run doc"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@swc/core": "^1.2.114",
    "@swc/jest": "^0.2.11",
    "@tmcw/togeojson": "5.0.0-3",
    "@types/geojson": "^7946.0.8",
    "@types/jest": "^27.0.3",
    "eslint": "^8.3.0",
    "jest": "^27.4.2",
    "prettier": "^2.5.1",
    "rollup": "^2.70.1",
    "rollup-plugin-dts": "^4.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.3.1",
    "typedoc": "^0.22.13",
    "typescript": "^4.5.2",
    "unist-builder": "^3.0.0",
    "xast-util-to-xml": "^3.0.0",
    "xastscript": "^3.0.0"
  }
}
